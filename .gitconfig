[user]
	name = Mark McKinstry
[color]
	ui = auto
[alias]
	co = checkout
	st = status
	br = branch
	bra = branch --all
	tree = log --graph --oneline
	fu = fetch --v upstream
	fo = fetch --v origin
	rum = reset --hard upstream/master
	rup = reset --hard upstream/production
	rom = reset --hard origin/master
	rop = reset --hard origin/production
[core]
	editor = emacs
[merge]
	stat = true
	conflictstyle = diff3
[push]
	default = simple
[rebase]
	autoStash = true
[status]
	short = true
	branch = true
