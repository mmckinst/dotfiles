[user]
	name = Mark McKinstry
[color]
	ui = auto
[alias]
	co = checkout
	st = status
	br = branch
	bra = branch --all
	tree = log --graph --oneline
[core]
	editor = emacs
[merge]
	stat = true
	conflictstyle = diff3
[push]
	default = simple
[rebase]
	autoStash = true
[status]
	short = true
	branch = true
